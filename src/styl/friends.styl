.onlineFriends
  height 50%
  > header
    > span
      font-weight bold
  > .popup_content
    overflow-y auto
    -webkit-overflow-scrolling touch
    border-radius 0 0 5px 5px
    .list_item
      border-bottom-color #eee
    .friend_tv
      float: right
      margin-right: 20px
    > .nofriend
      width 100%
      height 100%
      display flex
      align-items center
      justify-content center
  @media only screen and (min-height 700px)
    height 40%
