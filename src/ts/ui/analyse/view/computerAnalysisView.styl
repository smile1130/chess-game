.analyse-acplChart
  background linear-gradient(to bottom, rgba(255, 255, 255, 0) 0%, rgba(255, 255, 255, 0) 20%, rgba(128, 128, 128, 0.2) 50%, rgba(255, 255, 255, 0) 80%, rgba(255, 255, 255, 0) 100%)
  .area.above
    fill: rgba(#fff, 0.7)
  .area.below
    fill: #000
  .line
    stroke orange
  .zeroline
    stroke rgba(#fff, 0.3)
  .division.Opening
    stroke rgb(48, 204, 77)
  .division.Middlegame
    stroke rgb(48, 147, 204)
  .division.Endgame
    stroke rgb(204, 151, 48)
  .chart-legend
    font-size 0.5em
    fill rgba(#fff, 0.6)
