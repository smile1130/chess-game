{
    "name": "lichobile",
    "version": "6.3.0",
    "description": "lichess.org mobile application",
    "main": "app.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/veloce/lichobile.git"
    },
    "keywords": [
        "lichess",
        "chess",
        "application",
        "mobile",
        "realtime",
        "android",
        "ios"
    ],
    "author": "Vincent Velociter (https://github.com/veloce)",
    "license": "GPL-3.0-or-later",
    "bugs": {
        "url": "https://github.com/veloce/lichobile/issues"
    },
    "homepage": "https://github.com/veloce/lichobile",
    "browserslist": "and_chr >= 58, ios_saf >= 11",
    "devDependencies": {
        "@babel/preset-env": "7.6.2",
        "@types/jest": "24.0.11",
        "babel-plugin-transform-remove-console": "6.9.4",
        "babelify": "10.0.0",
        "browserify": "14.5.0",
        "chalk": "2.4.2",
        "concurrently": "5.0.0",
        "fancy-log": "1.3.3",
        "gulp": "4.0.2",
        "gulp-autoprefixer": "7.0.0",
        "gulp-preprocess": "3.0.2",
        "gulp-rename": "1.2.2",
        "gulp-sourcemaps": "2.4.0",
        "gulp-stylus": "2.7.0",
        "gulp-uglify-es": "1.0.1",
        "jest": "24.7.1",
        "minimist": "1.2.0",
        "ncp": "2.0.0",
        "rollup": "1.23.1",
        "rollup-plugin-commonjs": "10.1.0",
        "rollup-plugin-json": "4.0.0",
        "rollup-plugin-node-resolve": "5.2.0",
        "rollup-plugin-terser": "5.1.2",
        "tsify": "3.0.4",
        "tslint": "5.8.0",
        "typescript": "3.4.5",
        "vinyl-buffer": "1.0.1",
        "vinyl-source-stream": "2.0.0",
        "watchify": "3.11.1",
        "whatwg-fetch": "3.0.0"
    },
    "dependencies": {
        "@capacitor/android": "1.2.1",
        "@capacitor/cli": "1.2.1",
        "@capacitor/core": "1.2.1",
        "@capacitor/ios": "1.2.1",
        "@types/d3": "4.5.0",
        "@types/hammerjs": "2.0.35",
        "@types/lodash-es": "4.17.3",
        "@types/mithril": "2.0.0",
        "capacitor-cordova-plugin-stockfish": "git+https://github.com/veloce/cordova-plugin-stockfish.git#capacitor",
        "capacitor-sound-effect": "file:../capacitor-sound-effect",
        "cordova-plugin-fullscreen": "1.2.0",
        "cordova-plugin-insomnia": "4.2.0",
        "cordova-plugin-webview-checker": "1.0.0",
        "d3-axis": "1.0.8",
        "d3-scale": "1.0.6",
        "d3-selection": "1.1.0",
        "d3-shape": "1.2.0",
        "d3-time": "1.0.7",
        "d3-time-format": "2.0.5",
        "hammerjs": "2.0.8",
        "localforage": "1.5.5",
        "lodash": "4.17.15",
        "lodash-es": "4.17.15",
        "mithril": "2.0.4",
        "moment": "2.21.0",
        "onesignal-cordova-plugin": "2.5.2",
        "rlite-router": "1.1.3",
        "siema": "1.5.1",
        "signals": "1.0.0",
        "zanimo": "git+https://github.com/veloce/Zanimo.git"
    },
    "scripts": {
        "version": "node scripts/bump_version.js",
        "lint": "tslint --project tsconfig.json -e '**/dts/**' -e '**/*.js'",
        "test": "jest",
        "compile": "tsc --outDir build/",
        "compile-watch": "tsc -w --outDir build/",
        "build-web": "rollup -c",
        "watch-web": "rollup --silent -w -c",
        "watch": "npm run compile && concurrently 'npm:compile-watch' 'npm:watch-web'",
        "prebuild": "npm run lint && npm run test",
        "build": "gulp --target='capacitor' && npx cap copy",
        "build-stage": "npm run build -- --env=env-stage.json",
        "build-prod": "npm run build -- --env=env-prod.json",
        "build-release": "BABEL_ENV=production npm run build -- --mode='release' --env=env-prod.json"
    },
    "cordova": {
        "platforms": [],
        "plugins": {
            "cordova-plugin-insomnia": {},
            "cordova-plugin-fullscreen": {},
            "onesignal-cordova-plugin": {},
            "com.rjfun.cordova.plugin.lowlatencyaudio": {},
            "cordova-plugin-stockfish": {},
            "cordova-plugin-webview-checker": {}
        }
    },
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "transform": {
            "^.+\\.(ts|tsx)$": "<rootDir>/testPreprocessor.js"
        },
        "testMatch": [
            "**/__tests__/*.(ts|tsx|js)"
        ],
        "verbose": true
    }
}
