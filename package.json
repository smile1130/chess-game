{
    "name": "lichobile",
    "version": "6.1.4",
    "description": "lichess.org mobile application",
    "main": "app.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/veloce/lichobile.git"
    },
    "keywords": [
        "lichess",
        "chess",
        "application",
        "mobile",
        "realtime",
        "android",
        "ios"
    ],
    "author": "Vincent Velociter (https://github.com/veloce)",
    "license": "GPL-3.0-or-later",
    "bugs": {
        "url": "https://github.com/veloce/lichobile/issues"
    },
    "homepage": "https://github.com/veloce/lichobile",
    "devDependencies": {
        "@types/jest": "24.0.11",
        "babel-core": "6.26.3",
        "babel-plugin-transform-react-jsx": "6.24.1",
        "babel-plugin-transform-react-require": "1.0.1",
        "babel-plugin-transform-remove-console": "6.8.0",
        "babel-preset-env": "1.6.1",
        "babel-preset-es2015": "6.24.1",
        "babelify": "8.0.0",
        "browserify": "14.5.0",
        "chalk": "2.4.2",
        "fancy-log": "1.3.3",
        "gulp": "4.0.2",
        "gulp-autoprefixer": "6.1.0",
        "gulp-preprocess": "2.0.0",
        "gulp-rename": "1.2.2",
        "gulp-sourcemaps": "2.4.0",
        "gulp-stylus": "2.7.0",
        "gulp-uglify-es": "1.0.1",
        "jest": "24.7.1",
        "minimist": "1.2.0",
        "ncp": "2.0.0",
        "tsify": "3.0.4",
        "tslint": "5.8.0",
        "typescript": "3.4.5",
        "vinyl-buffer": "1.0.1",
        "vinyl-source-stream": "2.0.0",
        "watchify": "3.11.1"
    },
    "dependencies": {
        "@types/d3": "4.5.0",
        "@types/hammerjs": "2.0.35",
        "cordova-android": "8.0.0",
        "cordova-ios": "5.0.1",
        "cordova-plugin-appversion": "1.0.0",
        "cordova-plugin-crosswalk-data-migration": "git+https://github.com/veloce/cordova-plugin-crosswalk-data-migration.git#225408d",
        "cordova-plugin-device": "2.0.2",
        "cordova-plugin-dialogs": "2.0.1",
        "cordova-plugin-file": "6.0.1",
        "cordova-plugin-file-transfer": "1.7.1",
        "cordova-plugin-fullscreen": "1.2.0",
        "cordova-plugin-globalization": "1.11.0",
        "cordova-plugin-inappbrowser": "1.6.1",
        "cordova-plugin-insomnia": "4.2.0",
        "cordova-plugin-nativeaudio": "github:veloce/cordova-plugin-nativeaudio#old_interface",
        "cordova-plugin-network-information": "2.0.1",
        "cordova-plugin-splashscreen": "5.0.2",
        "cordova-plugin-statusbar": "2.4.2",
        "cordova-plugin-stockfish": "git+https://github.com/veloce/cordova-plugin-stockfish.git#v1.8.0",
        "cordova-plugin-vibration": "3.1.0",
        "cordova-plugin-whitelist": "1.3.3",
        "cordova-plugin-x-socialsharing": "5.1.3",
        "cordova-plugin-x-toast": "2.7.2",
        "cordova-universal-links-plugin": "git+https://github.com/veloce/cordova-universal-links-plugin.git#c494fe1",
        "core-js": "2.5.7",
        "d3-axis": "1.0.8",
        "d3-scale": "1.0.6",
        "d3-selection": "1.1.0",
        "d3-shape": "1.2.0",
        "d3-time": "1.0.7",
        "d3-time-format": "2.0.5",
        "hammerjs": "2.0.8",
        "ionic-plugin-keyboard": "2.2.1",
        "localforage": "1.5.5",
        "lodash": "4.17.11",
        "mithril": "github:MithrilJS/mithril.js#v2.0.0-rc.4",
        "moment": "2.21.0",
        "onesignal-cordova-plugin": "2.4.7",
        "rlite-router": "1.1.3",
        "siema": "1.5.1",
        "signals": "1.0.0",
        "whatwg-fetch": "2.0.3",
        "zanimo": "git+https://github.com/veloce/Zanimo.git"
    },
    "scripts": {
        "version": "node scripts/bump_version.js",
        "lint": "tslint --project tsconfig.json -e '**/dts/**' -e '**/*.js'",
        "build": "gulp",
        "test": "jest",
        "watch": "gulp watch",
        "watch-dev": "gulp watch --env=env-dev.json",
        "watch-stage": "gulp watch --env=env-stage.json",
        "watch-prod": "gulp watch --env=env-prod.json",
        "build-stage": "BABEL_ENV=production gulp --mode='release' --env=env-stage.json",
        "prebuild-stage": "npm run lint && npm run test",
        "build-cordova-dev": "gulp --target='cordova' --env=env-dev.json",
        "build-cordova-stage": "gulp --target='cordova' --env=env-stage.json",
        "build-cordova-prod": "gulp --target='cordova' --env=env-prod.json",
        "build-cordova-release": "BABEL_ENV=production gulp --mode='release' --target='cordova' --env=env-prod.json",
        "android-dev": "npm run build-cordova-dev && cordova run android --browserify",
        "android-stage": "npm run build-cordova-stage && cordova run android --browserify",
        "android-prod": "npm run build-cordova-prod && cordova run android --browserify",
        "android-release": "npm run build-cordova-release && cordova run android --browserify",
        "ios-dev": "npm run build-cordova-dev && cordova run ios --browserify",
        "ios-stage": "npm run build-cordova-stage && cordova run ios --browserify",
        "ios-prod": "npm run build-cordova-prod && cordova run ios --browserify",
        "prepare-ios-dev": "npm run build-cordova-dev && cordova prepare ios --browserify",
        "prepare-ios-stage": "npm run build-cordova-stage && cordova prepare ios --browserify",
        "prepare-ios-prod": "npm run build-cordova-prod && cordova prepare ios --browserify",
        "release-android": "npm run build-cordova-release && cordova build android --release --browserify",
        "release-android-4.4": "npm run build-cordova-release && cordova build android --release --browserify -- --minSdkVersion=19",
        "release-ios": "npm run build-cordova-release && cordova build ios --release --browserify"
    },
    "cordova": {
        "platforms": [
            "ios",
            "android"
        ],
        "plugins": {
            "cordova-plugin-appversion": {},
            "cordova-plugin-device": {},
            "cordova-plugin-inappbrowser": {},
            "cordova-plugin-insomnia": {},
            "cordova-plugin-x-socialsharing": {},
            "cordova-plugin-network-information": {},
            "cordova-plugin-file": {},
            "cordova-universal-links-plugin": {},
            "cordova-plugin-dialogs": {},
            "cordova-plugin-whitelist": {},
            "cordova-plugin-vibration": {},
            "cordova-plugin-statusbar": {},
            "cordova-plugin-splashscreen": {},
            "cordova-plugin-fullscreen": {},
            "cordova-plugin-globalization": {},
            "cordova-plugin-crosswalk-data-migration": {},
            "ionic-plugin-keyboard": {},
            "cordova-plugin-stockfish": {},
            "cordova-plugin-x-toast": {},
            "cordova-plugin-file-transfer": {},
            "onesignal-cordova-plugin": {}
        }
    },
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "transform": {
            "^.+\\.(ts|tsx)$": "<rootDir>/testPreprocessor.js"
        },
        "testMatch": [
            "**/__tests__/*.(ts|tsx|js)"
        ],
        "verbose": true
    }
}
