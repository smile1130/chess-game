{
    "name": "lichobile",
    "version": "0.0.0",
    "description": "lichess.org mobile application",
    "main": "app.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/veloce/lichobile.git"
    },
    "keywords": [
        "lichess",
        "chess",
        "application",
        "mobile",
        "realtime",
        "android",
        "ios"
    ],
    "author": "Vincent Velociter (https://github.com/veloce)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/veloce/lichobile/issues"
    },
    "homepage": "https://github.com/veloce/lichobile",
    "devDependencies": {
        "babel-plugin-transform-react-jsx": "~6.8.0",
        "babel-plugin-transform-react-require": "~1.0.1",
        "babel-plugin-transform-remove-console": "~6.8.0",
        "gulp": "~3.9.1",
        "gulp-autoprefixer": "~3.1.1",
        "gulp-preprocess": "~2.0.0",
        "gulp-rename": "~1.2.2",
        "gulp-sourcemaps": "~2.4.0",
        "gulp-stylus": "~2.6.0",
        "gulp-uglify": "~2.0.0",
        "gulp-util": "~3.0.7",
        "minimist": "~1.2.0",
        "ncp": "~2.0.0",
        "tsify": "~2.0.3",
        "typescript": "~2.3.0",
        "uglify-js": "~2.7.5",
        "vinyl-buffer": "~1.0.0",
        "vinyl-source-stream": "~1.1.0",
        "yargs": "~1.3.1"
    },
    "dependencies": {
        "@types/d3": "^4.5.0",
        "babel-preset-es2015": "6.16.0",
        "babelify": "7.3.0",
        "browserify": "13.1.0",
        "chess.js": "git://github.com/veloce/chess.js#8f9b37d2",
        "d3-axis": "1.0.4",
        "d3-scale": "1.0.4",
        "d3-selection": "1.0.3",
        "d3-shape": "1.0.4",
        "d3-time": "1.0.4",
        "d3-time-format": "2.0.3",
        "iscroll": "5.1.3",
        "lodash": "4.17.4",
        "mithril": "lhorie/mithril.js#b466c7f",
        "moment": "2.14.1",
        "raven-js": "3.15.0",
        "rlite-router": "1.1.3",
        "signals": "1.0.0",
        "watchify": "3.7.0",
        "whatwg-fetch": "1.0.0",
        "zanimo": "git://github.com/veloce/Zanimo",
        "cordova-ios": "~4.3.1",
        "cordova-android": "~6.2.2",
        "com.rjfun.cordova.plugin.lowlatencyaudio": "https://github.com/veloce/cordova-plugin-nativeaudio.git#old_interface",
        "cordova-plugin-appversion": "~1.0.0",
        "cordova-plugin-crosswalk-webview": "~2.3.0",
        "cordova-plugin-customurlscheme": "~4.1.3",
        "cordova-plugin-device": "~1.1.4",
        "cordova-plugin-dialogs": "~1.3.1",
        "cordova-plugin-file-transfer": "~1.6.1",
        "cordova-plugin-fullscreen": "~1.1.0",
        "cordova-plugin-globalization": "~1.0.5",
        "cordova-plugin-inappbrowser": "~1.6.1",
        "cordova-plugin-insomnia": "~4.2.0",
        "cordova-plugin-network-information": "~1.3.1",
        "cordova-plugin-splashscreen": "~4.0.1",
        "cordova-plugin-statusbar": "~2.2.1",
        "cordova-plugin-stockfish": "~1.3.0",
        "cordova-plugin-vibration": "~2.1.3",
        "cordova-plugin-whitelist": "~1.3.1",
        "cordova-plugin-x-socialsharing": "~5.1.3",
        "cordova-plugin-x-toast": "~2.5.2",
        "cordova-universal-links-plugin": "~1.2.1",
        "ionic-plugin-keyboard": "~2.2.1",
        "onesignal-cordova-plugin": "~2.0.11"
    },
    "scripts": {
        "lint": "tslint --project tsconfig.json -e '**/dts/**' -e '**/*.js'",
        "build": "gulp",
        "watch": "gulp watch",
        "watch-dev": "gulp watch --env=env-dev.json",
        "watch-stage": "gulp watch --env=env-stage.json",
        "watch-prod": "gulp watch --env=env-prod.json",
        "build-js-dev": "gulp --target='cordova' --env=env-dev.json",
        "build-js-stage": "gulp --target='cordova' --env=env-stage.json",
        "build-js-prod": "gulp --target='cordova' --env=env-prod.json",
        "build-js-release": "BABEL_ENV=production gulp --mode='release' --target='cordova' --env=env-prod.json",
        "android-dev": "npm run build-js-dev && cordova run android --browserify",
        "android-stage": "npm run build-js-stage && cordova run android --browserify",
        "android-prod": "npm run build-js-prod && cordova run android --browserify",
        "android-release": "npm run build-js-release && cordova run android --browserify",
        "ios-dev": "npm run build-js-dev && cordova run ios --browserify",
        "ios-stage": "npm run build-js-stage && cordova run ios --browserify",
        "ios-prod": "npm run build-js-prod && cordova run ios --browserify",
        "prepare-ios-dev": "npm run build-js-dev && cordova prepare ios --browserify",
        "prepare-ios-stage": "npm run build-js-stage && cordova prepare ios --browserify",
        "prepare-ios-prod": "npm run build-js-prod && cordova prepare ios --browserify",
        "release-android": "npm run build-js-release && cordova build android --release --browserify",
        "release-ios": "npm run build-js-release && cordova build ios --release --browserify"
    },
    "cordova": {
        "platforms": [
            "android",
            "ios"
        ],
        "plugins": {
            "com.rjfun.cordova.plugin.lowlatencyaudio": {},
            "cordova-plugin-appversion": {},
            "cordova-plugin-crosswalk-webview": {
                "XWALK_MODE": "embedded",
                "XWALK_MULTIPLEAPK": "true"
            },
            "cordova-plugin-customurlscheme": {
                "URL_SCHEME": "lichess"
            },
            "cordova-plugin-device": {},
            "cordova-plugin-dialogs": {},
            "cordova-plugin-file-transfer": {},
            "cordova-plugin-fullscreen": {},
            "cordova-plugin-globalization": {},
            "cordova-plugin-inappbrowser": {},
            "cordova-plugin-insomnia": {},
            "cordova-plugin-network-information": {},
            "cordova-plugin-splashscreen": {},
            "cordova-plugin-statusbar": {},
            "cordova-plugin-stockfish": {},
            "cordova-plugin-vibration": {},
            "cordova-plugin-whitelist": {},
            "cordova-plugin-x-socialsharing": {},
            "cordova-plugin-x-toast": {},
            "cordova-universal-links-plugin": {},
            "ionic-plugin-keyboard": {},
            "onesignal-cordova-plugin": {}
        }
    }
}
