// See https://github.com/jshint/jshint/blob/2.x/examples/.jshintrc
{
  "globals": {
    "window": true,
    "document": true,
    "m": true
  },
  "esnext": true,
  "node": true,
  "bitwise": true,
  "camelcase": false,
  "newcap": false,
  "eqeqeq": true,
  "immed": true,
  "latedef": "nofunc",
  "undef": true,
  "unused": true,
  "trailing": true,
  "smarttabs": false,
  "forin": false
}
