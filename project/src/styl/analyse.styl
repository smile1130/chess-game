.analyse
  width 100%
  @media only screen and (orientation: portrait)
    height calc(100vh - 100vw - 50px - 45px)
    display flex
    flex-flow row wrap
    align-items flex-start
    overflow-x hidden
    turn
      display inline
      margin-right 10px

    index
      display inline
      margin-right 5px

    move
      display inline
      margin 0 2px
