.seeks
  height 75%
  position relative

.seeks_scroller
  margin-bottom 20px
  height 100%
  overflow hidden
  .must_signin
    text-align center

.seek
  overflow hidden
  &.cancel
    color #C4A86F
  .icon::before
    display block
    float left
    font-size 40px
    opacity 0.5
  .body
    line-height 22px
    margin-left 60px

.seeks
  button.fat
    display block
    max-height 20%
    width 80%
    background #C4A86F
    color #fff
    padding 15px
    font-size 15px
    margin 0 auto
