{
  "parser": "babel-eslint",
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "globals": {
      "window": true,
      "cordova": true,
      "Stockfish": true
    },
    "rules": {
      "quotes": [ 2, "single" ],
      "linebreak-style": [ 2, "unix" ],
      "no-console": 0,
      "no-alert": 0,
      "no-use-before-define": [1, "nofunc"],
      "no-array-constructor": 2,
      "no-caller": 2,
      "no-catch-shadow": 2,
      "no-eval": 2,
      "no-extend-native": 2,
      "no-extra-bind": 2,
      "no-implied-eval": 2,
      "no-iterator": 2,
      "no-label-var": 2,
      "no-labels": 2,
      "no-lone-blocks": 2,
      "no-loop-func": 0,
      "no-multi-spaces": 0,
      "no-multi-str": 2,
      "no-native-reassign": 2,
      "no-new": 2,
      "no-new-func": 2,
      "no-new-object": 2,
      "no-new-wrappers": 2,
      "no-octal-escape": 2,
      "no-process-exit": 2,
      "no-proto": 2,
      "no-return-assign": 2,
      "no-script-url": 2,
      "no-sequences": 2,
      "no-shadow": 2,
      "no-shadow-restricted-names": 2,
      "no-spaced-func": 0,
      "no-trailing-spaces": 2,
      "no-undef-init": 2,
      "no-underscore-dangle": 0,
      "no-unused-expressions": 2,
      "no-with": 2,
      "comma-spacing": 2,
      "consistent-return": 2,
      "dot-notation": [2, { "allowKeywords": true }],
      "eol-last": 2,
      "no-extra-parens": [2, "functions"],
      "eqeqeq": 2,
      "key-spacing": [2, { "beforeColon": false, "afterColon": true }],
      "new-cap": 0,
      "new-parens": 2,
      "semi": [ 2, "always" ],
      "semi-spacing": [2, {"before": false, "after": true}],
      "space-infix-ops": 0,
      "keyword-spacing": 2,
      "space-unary-ops": [2, { "words": true, "nonwords": false }]
    },
    "extends": "eslint:recommended"
}
